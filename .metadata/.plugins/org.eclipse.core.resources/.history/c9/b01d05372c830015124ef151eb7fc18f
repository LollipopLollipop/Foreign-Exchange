import java.io.*;
public class DeserializeDemo
{
   public static void main(String [] args)
   {
      CandleStick e = null;
      try
      {
         FileInputStream fileIn = new FileInputStream("cs.ser");
         ObjectInputStream in = new ObjectInputStream(fileIn);
         e = (CandleStick) in.readObject();
         in.close();
         fileIn.close();
      }catch(IOException i)
      {
         i.printStackTrace();
         return;
      }catch(ClassNotFoundException c)
      {
         System.out.println("Employee class not found");
         c.printStackTrace();
         return;
      }
      System.out.println("Deserialized CS...");
      System.out.println("CurrencyPair: " + e.getCurrencyPair());
      System.out.println("Time: " + e.getTime());
      System.out.println("High: " + e.SSN);
      System.out.println("Number: " + e.number);
    }
}